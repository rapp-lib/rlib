        "vlucas/phpdotenv": "v2.*@stable",
        "phpmailer/phpmailer": "v5.*@stable",
        "zendframework/zend-cache": "v2.*@stable",
        "zendframework/zend-session": "v2.*@stable",
        "deepfreeze/zend-diactoros": "v1.*@stable",
