{
    "name": "rapp-lib/rlib",
    "description": "Simple webapp library",
    "type": "library",
    "license": "MIT",
    "require": {
        "smarty/smarty": "v3.*@stable",
        "doctrine/dbal": "v2.*@stable",
        "monolog/monolog": "v1.*@stable",
        "vlucas/phpdotenv": "v2.*@stable",
        "phpmailer/phpmailer": "v5.*@stable"
    },
    "suggest": {
        "zendframework/zend-diactoros": "4.0 @PHP5.4 support",
        "nikic/fast-route": "4.0 @PHP5.4 support",
        "mindplay/middleman": "4.0 @PHP5.4 support"
    },
    "autoload": {
        "psr-4": {
            "R\\Lib\\": "lib/"
        },
        "files": [
            "functions/required.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "master-3.2",
            "dev-develop": "master-4.0"
        }
    }
}
