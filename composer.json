{
    "name": "rapp-lib/rlib",
    "description": "Laravel based PSR mixture stack.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=5.3",
        "laravel/framework": "~4.1",
        "zendframework/zend-session": "~2",
        "zendframework/zend-serializer": "~2",
        "zendframework/zend-cache": "~2",
        "danbruce/fast-route": "~0.6",
        "doctrine/dbal": "~2",
        "smarty/smarty": "~3",
        "monolog/monolog": "~1",
        "guzzlehttp/guzzle": "~3",
        "phpunit/phpunit": "~4",
        "vlucas/phpdotenv": "~2",
        "phpmailer/phpmailer": "~5",
        "airmanbzh/php-html-generator": "~1",
        "rapp-lib/zend-diactoros": "~1",
        "rapp-lib/laravel-debugbar": "~1",
        "rapp-lib/middleman": "~2"
    },
    "autoload": {
        "psr-4": {
            "R\\Lib\\": "lib/"
        },
        "files": [
            "functions/required.php"
        ]
    }
}
