    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=5.3.0",
        "smarty/smarty": "v3.*@stable",
        "doctrine/dbal": "v2.*@stable",
        "monolog/monolog": "v1.*@stable",
        "vlucas/phpdotenv": "v2.*@stable",
        "phpmailer/phpmailer": "v5.*@stable",
        "symfony/polyfill-apcu": "~1.1",
        "symfony/polyfill-intl-icu": "~1.0",
