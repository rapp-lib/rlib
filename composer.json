{
	"name": "rapp-lib/rlib",
	"description": "Simple webapp library",
	"type": "library",
	"license": "MIT",
	"require": {
		"smarty/smarty": "v3.*@stable",
		"swiftmailer/swiftmailer": "v5.*@stable"
	},
	"autoload": {
		"psr-4": {
			"R\\Lib\\": "lib/",
			"R\\Plugin\\": "plugin/",
			"R\\Util\\": "util/"
		},
		"files": [
			"core/array.php",
			"core/html.php",
			"core/path.php",
			"core/webapp.php",
			"core/date.php",
			"core/string.php",
			"core/file.php",
			"core/modules.php",
			"core/report.php",
			"core/vars.php",
			"core/lib_db.php",
			"core/lib_smarty.php"
		]
	},
	"include-path":[
		"ext/include/",
		"ext/pear/",
		"lib_db/",
		"lib_smarty/",
		"lib_context/"
	]
}