-- 管理者更新ログテーブル 
CREATE TABLE admin_log (
	`LOG_ID` text COMMENT 'ログID(int)',
	`ADMIN_ID` text COMMENT '管理者ID(int)',
	`KOUSIN_TABLE` text COMMENT '更新対象テーブル(text)',
	`KOUSIN_INFO` text COMMENT '更新内容(int)',
	`KOUSIN_TIME` text COMMENT '更新日時(int)'	)	;

-- 管理者ID 
CREATE TABLE admin_table (
	`ADMIN_ID` text COMMENT '管理者ID(int)',
	`LOGIN_ID` text COMMENT '管理者ログインID(text)',
	`LOGIN_PASSWORD` text COMMENT '管理者パスワード(text)',
	`KENGEN` text COMMENT '管理者権限(int)',
	`LOGIN_DATE` text COMMENT '最終ログイン日時(int)',
	`UPDATE_DATE` text COMMENT '更新日時(int)',
	`REG_DATE` text COMMENT '登録日時(int)',
	`DEL_FLG` text COMMENT '削除フラグ(int)'	)	;

